    <string name="faster_ball_help">"Poveča hitrost žogic(e) za 25%."</string>
    <string name="slower_ball_help">"Zmanjša hitrost žogic(e) za 25%."</string>
    <string name="fireball_help">"Spremeni vse trenutno aktivne žogice v ognjene krogle. Ognjene krogle prodrejo skozi vse zidove (razen skozi neuničljive)."</string>
