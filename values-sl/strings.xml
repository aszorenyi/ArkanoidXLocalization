    <string name="game_goal_title">"Cilj igre"</string>
    <string name="game_goal">"Poskusi razbiti vse zidove na posamezni stopnji tako, da zbereš čim več točk in napreduješ na naslednjo stopnjo."</string>
    <string name="basic_rules_title">"Osnovna pravila"</string>
    <string name="basic_rules">"Premikaj lopar(je) levo in desno tako, da odbijaš žogico/-e. Vsaka stopnja je končana, ko so razbiti vsi uničljivi zidovi za zaslonu. Igralec lahko z loparjem spremeni kot, pod katerim se žogica odbije. Od zidov se žogica odbije pod istim kotom in z isto hitrostjo, s kakršnima je zid zadela."</string>
    <string name="life_rules_title">"Življenja"</string>
    <string name="life_rules">"Igralec začne igro s 3 življenji. Igra je končana, ko se žogica dotakne dna (oz. vrha na nekaterih stopnjah) zaslona in igralec nima več življenj."</string>
    <string name="points_title">"Točke"</string>
    <string name="bricks_title">"Zidovi"</string>
    <string name="bricks">"V igri so štiri različne vrste zidov."</string>
    <string name="normal_and_hard_bricks_title">"Navadni in trdi zidovi"</string>
    <string name="normal_and_hard_bricks">"Ti zidovi so lahko navadni ali trdi. Trdi zidovi so običajno temnejše barve in jih je potrebno udariti dvakrat, da se jih razbije."</string>
    <string name="unbreakable_bricks_title">"Neuničljivi in premikajoči se zidovi"</string>
    <string name="unbreakable_bricks">"Te vrste zidov se ne more uničiti."</string>
